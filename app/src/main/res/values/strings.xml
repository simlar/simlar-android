    <!-- Allow Simlar to record audio? -->
    <string name="permission_explain_text_record_audio">Simlar uses your microphone during Simlar calls so that the person you are talking to can hear you.</string>
    <!-- > Allow Simlar to make and manage phone calls? -->
    <string name="permission_explain_text_phone_number">During registration Simlar may read your telephone number automatically.</string>
    <!-- > Allow Simlar to make and manage phone calls? -->
    <string name="permission_explain_text_phone_state">During Simlar calls the app watches your phone state to manage incoming regular phone calls.</string>
    <!-- > Allow Simlar to make and manage phone calls? -->
    <string name="permission_explain_text_phone_state_with_phone_number">During registration Simlar may read your telephone number automatically.\nDuring Simlar calls the app watches your phone state to manage incoming regular phone calls.</string>
    <!-- Allow Simlar to access your contacts? -->
    <string name="permission_explain_text_contacts">Simlar reads your contacts data to show the contacts you can talk to privately.</string>
    <!-- Allow Simlar to take pictures and record video? -->
    <string name="permission_explain_text_camera">Simlar uses your camera during Simlar video calls so that the person you are talking to can see you.</string>
    <!-- Allow Simlar to send you notifications -->
    <string name="permission_explain_text_post_notification">With notifications Simlar informs you about incoming calls.</string>

    <!-- Do Not Disturb -->
    <!-- Allow access to Do Not Disturb for Simlar? -->
    <!-- The app will be able to turn on/off Do Not Disturb and make changes to related settings. -->
    <string name="permission_explain_text_notification_policy_access">Simlar wants to enter Do Not Disturb mode during calls in order to prevent e.g. other calls from vibrating and ringing.</string>
    <string name="permission_explain_text_notification_policy_access_button">Open Settings</string>

